INPUT                  = @DOXYGEN_INPUT_DIR@
GENERATE_TREEVIEW      = YES
DISABLE_INDEX          = NO
FULL_SIDEBAR           = NO
HTML_EXTRA_STYLESHEET  = "@CMAKE_CURRENT_SOURCE_DIR@/doxygen-awesome.css" "@CMAKE_CURRENT_SOURCE_DIR@/doxygen-awesome-sidebar-only.css" "@CMAKE_CURRENT_SOURCE_DIR@/doxygen-awesome-sidebar-only-darkmode-toggle.css"
HTML_COLORSTYLE        = LIGHT
HTML_HEADER            = "@CMAKE_CURRENT_SOURCE_DIR@/header.html"
HTML_EXTRA_FILES       = "@CMAKE_CURRENT_SOURCE_DIR@/doxygen-awesome-darkmode-toggle.js" "@CMAKE_CURRENT_SOURCE_DIR@/doxygen-awesome-fragment-copy-button.js"
RECURSIVE              = YES

# Project related stuff
PROJECT_NAME           = "@PROJECT_NAME@"
PROJECT_NUMBER         = "@PROJECT_VERSION@"
PROJECT_BRIEF          = "@PROJECT_DESCRIPTION@"

# other stuff
GENERATE_LATEX         = NO